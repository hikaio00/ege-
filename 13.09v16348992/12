for x in range(1, 50):
    for y in range(1, 50):
        for z in range(1, 50):
            s = '0' + x * '1' + y * '2' + z * '3'
            while ('01' in s) or ('02' in s) or ('03' in s):
                s = s.replace('01', '2302', 1)
                s = s.replace('02', '10', 1)
                s = s.replace('03', '201', 1)
            if s.count('1') == 40 and s.count('2') == 10 and s.count('3') == 8:
                print(x)
